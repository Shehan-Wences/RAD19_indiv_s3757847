<% provide(:title, 'Create a Location') %>
<h1>Sign up</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@location, url: locationcreate_path) do |f| %>
      <%#= render 'shared/error_messages' %>

      <%= f.label 'Location' %>
      <%= f.text_field :locationname, class: 'form-control' %>



      <%= f.submit "Create the Location", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>